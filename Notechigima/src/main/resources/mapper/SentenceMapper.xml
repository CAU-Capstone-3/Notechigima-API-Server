<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.capstone.notechigima.repository.SentenceRepository">

    <select id="getSentenceList" resultType="com.capstone.notechigima.model.dao.sentence.SentenceEntity">
        SELECT *
        FROM sentence_unit s
        WHERE #{noteId} = s.note_id
    </select>

</mapper>